[user]
	name = Michael Scheiwiller
	email = mi.lappert@gmail.com
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	st = status
	co = checkout
	br = branch
	last = log -1 HEAD
	unstage = reset HEAD --
	lg = log --oneline --graph --decorate -10
[pull]
	rebase = true
